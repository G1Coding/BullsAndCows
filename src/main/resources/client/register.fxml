<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.Controller">
    <children>
        <ImageView fitHeight="650.0" fitWidth="1000.0" layoutX="-6.0" pickOnBounds="true">
            <image>
                <Image url="@../images/register.jpg" />
            </image>
        </ImageView>
        <AnchorPane layoutX="122.0" layoutY="218.0">
            <children>
                <ToggleButton fx:id="registerEndBtn" layoutX="101.0" layoutY="307.0" mnemonicParsing="false" onAction="#registerBtnCliked" prefHeight="40.0" prefWidth="290.0" text="회원가입" />
                <GridPane prefHeight="267.0" prefWidth="406.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="301.0" minWidth="10.0" prefWidth="264.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="193.0" minWidth="10.0" prefWidth="142.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <TextField fx:id="txtName" onAction="#registerBtnCliked" prefHeight="24.0" prefWidth="159.0" GridPane.columnIndex="1" />
                        <DatePicker fx:id="userBirth" onAction="#registerBtnCliked" prefHeight="30.0" prefWidth="131.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <PasswordField fx:id="txtRePassword" onAction="#registerBtnCliked" prefHeight="25.0" prefWidth="203.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <PasswordField fx:id="txtPassword" onAction="#registerBtnCliked" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <TextField fx:id="txtId" onAction="#registerBtnCliked" prefHeight="30.0" prefWidth="167.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label alignment="CENTER" prefHeight="19.0" prefWidth="269.0" text="닉네임" />
                        <Label alignment="CENTER" prefHeight="19.0" prefWidth="275.0" text="생년월일" GridPane.rowIndex="1" />
                        <Label alignment="CENTER" prefHeight="19.0" prefWidth="261.0" text="아이디" GridPane.rowIndex="2" />
                        <Label alignment="CENTER" prefHeight="19.0" prefWidth="290.0" text="비밀번호" GridPane.rowIndex="3" />
                        <Label alignment="CENTER" prefHeight="19.0" prefWidth="274.0" text="비밀번호 확인" GridPane.rowIndex="4" />
                    </children>
                </GridPane>
                <ToggleButton fx:id="checkIdBtn" layoutX="434.0" layoutY="119.0" mnemonicParsing="false" onAction="#isIdUsing" text="아이디 중복확인" />
            <ToggleButton fx:id="checkNameBtn" layoutX="434.0" layoutY="14.0" mnemonicParsing="false" onAction="#isNameUsing" text="닉네임 중복확인" />
            </children>
        </AnchorPane>
        <ToggleButton fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#moveBackPage" prefHeight="43.0" prefWidth="148.0" text="Login 하러 가기" />
        <Text layoutX="437.0" layoutY="127.0" strokeType="OUTSIDE" strokeWidth="0.0" text="회원가입">
            <font>
                <Font size="37.0" />
            </font>
        </Text>
    </children>
</AnchorPane>
